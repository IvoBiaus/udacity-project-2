{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "lint": "eslint 'src/*.ts'",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "node src/server.ts",
    "reset": "db-migrate reset:all && yarn migrate",
    "migrate": "db-migrate --env test up && db-migrate up",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "test": "db-migrate db:create wands_shop_test && db-migrate --env test up && jasmine-ts && db-migrate db:drop wands_shop_test",
    "remove-test": "db-migrate db:drop wands_shop_test",
    "tsc": "tsc",
    "connect": "sudo service postgresql start"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jasmine": "^3.9.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.9.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@types/supertest": "^2.0.11",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.0",
    "ts-node": "^10.2.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.3.5"
  }
}
